{
  "name": "react-media-player",
  "version": "0.5.0",
  "description": "React media player.",
  "main": "dist/react-media-player.min.js",
  "files": [
    "dist/",
    "example/",
    "src/",
    "lib/",
    "LICENSE",
    "bower.json",
    "package.json",
    "webpack.config.js",
    "webpack.prod.config.js",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build": "npm run build:lib && NODE_ENV=production webpack --config webpack.prod.config.js",
    "dev": "webpack-dev-server --devtool eval --hot --progress --colors",
    "postbuild": "NODE_ENV=production TARGET=minify webpack --config webpack.prod.config.js",
    "prebuild": "rm -rf dist && mkdir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/souporserious/react-media-player"
  },
  "keywords": [
    "react",
    "component",
    "media",
    "audio",
    "video",
    "player",
    "playlist"
  ],
  "author": "Travis Arnold <travis@souporserious.com> (http://souporserious.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/souporserious/react-media-player/issues"
  },
  "homepage": "https://github.com/souporserious/react-media-player",
  "peerDependencies": {
    "react": "0.14.x || ^15.0.0",
    "react-dom": "0.14.x || ^15.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.25.0",
    "http-server": "^0.9.0",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^3.2.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-motion-ui-pack": "^0.9.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.9.12",
    "webpack-dev-server": "^1.9.0"
  }
}
